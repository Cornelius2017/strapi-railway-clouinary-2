{
  "kind": "collectionType",
  "collectionName": "you_tubes",
  "info": {
    "singularName": "you-tube",
    "pluralName": "you-tubes",
    "displayName": "YouTube"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "content": {
      "type": "richtext"
    },
    "date": {
      "type": "datetime"
    },
    "slug": {
      "type": "uid",
      "targetField": "title",
      "required": true
    },
    "position": {
      "type": "enumeration",
      "enum": [
        "top"
      ]
    },
    "img": {
      "allowedTypes": [
        "images"
      ],
      "type": "media",
      "multiple": false
    },
    "video": {
      "allowedTypes": [
        "videos",
        "audios"
      ],
      "type": "media",
      "multiple": false
    },
    "categories": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::category.category",
      "inversedBy": "you_tubes"
    },
    "tags": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::tag.tag",
      "inversedBy": "you_tubes"
    },
    "authors": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "plugin::users-permissions.user",
      "mappedBy": "you_tube"
    }
  }
}
