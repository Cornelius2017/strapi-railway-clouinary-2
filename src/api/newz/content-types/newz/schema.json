{
  "kind": "collectionType",
  "collectionName": "news",
  "info": {
    "singularName": "newz",
    "pluralName": "news",
    "displayName": "News"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string"
    },
    "slug": {
      "type": "uid",
      "required": true,
      "targetField": "title"
    },
    "date": {
      "type": "datetime"
    },
    "img": {
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ],
      "type": "media",
      "multiple": false
    },
    "content": {
      "type": "richtext"
    },
    "position": {
      "type": "enumeration",
      "enum": [
        "news-1",
        "news-2",
        "news-3",
        "news-4"
      ]
    },
    "authors": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::category.category",
      "mappedBy": "news"
    },
    "categories": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::category.category",
      "inversedBy": "news"
    },
    "tags": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::category.category",
      "inversedBy": "news"
    }
  }
}
