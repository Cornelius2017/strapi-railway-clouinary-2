{
  "kind": "collectionType",
  "collectionName": "posts",
  "info": {
    "singularName": "post",
    "pluralName": "posts",
    "displayName": "Post",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "generated_content": {
      "type": "customField",
      "customField": "plugin::ai-text-generation.text-ai",
      "options": {
        "length": "250"
      }
    },
    "title": {
      "type": "string"
    },
    "slug": {
      "type": "customField",
      "customField": "plugin::custom-slug.slug"
    },
    "UUID": {
      "type": "customField",
      "customField": "plugin::field-uuid.uuid"
    }
  }
}
