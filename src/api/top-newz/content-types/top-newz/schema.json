{
  "kind": "collectionType",
  "collectionName": "top_news",
  "info": {
    "singularName": "top-newz",
    "pluralName": "top-news",
    "displayName": "[1] Top-News",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string"
    },
    "date": {
      "type": "datetime"
    },
    "content": {
      "type": "richtext"
    },
    "img": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "slug": {
      "type": "uid",
      "targetField": "title",
      "required": true
    },
    "authors": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "plugin::users-permissions.user"
    },
    "position": {
      "type": "enumeration",
      "enum": [
        "top-1",
        "top-2"
      ]
    },
    "categories": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::category.category",
      "inversedBy": "top_news"
    },
    "tags": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::tag.tag",
      "inversedBy": "top_news"
    }
  }
}
